<div class="container-fluid">
    <br>
    <br>
    <!-- Page Heading -->
    <div class="d-sm-flex align-items-center justify-content-between mb-4">
        <h1 class="h3 mb-0 text-gray-800">Dashboard</h1>
    </div>

    <!-- Content Row -->
    <div class="row">

        <!-- Earnings (Monthly) Card Example -->
        <div class="col-xl-3 col-md-6 mb-4">
            <div class="card border-left-primary shadow h-100 py-2">
                <div class="card-body">
                    <div class="row no-gutters align-items-center">
                        <div class="col mr-2">
                            <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">Total users</div>
                            <div class="h5 mb-0 font-weight-bold text-gray-800">{{totalUser}}</div>
                        </div>
                        <div class="col-auto">
                            <i class="fas fa-calendar fa-2x text-gray-300"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Earnings (Monthly) Card Example -->
        <div class="col-xl-3 col-md-6 mb-4">
            <div class="card border-left-success shadow h-100 py-2">
                <div class="card-body">
                    <div class="row no-gutters align-items-center">
                        <div class="col mr-2">
                            <div class="text-xs font-weight-bold text-success text-uppercase mb-1">Joined today </div>
                            <div class="h5 mb-0 font-weight-bold text-gray-800">{{joinedToday.length}}</div>
                        </div>
                        <div class="col-auto">
                            <i class="fas fa-dollar-sign fa-2x text-gray-300"></i>
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </div>


    <div class="row">
        <div class="col-sm-12 mb-4">
            <h1 class="h3 mb-0 text-gray-800">Trending Memes</h1>
              {{#each memeTrendingArray}}
            <span class="badge badge-primary">{{this}}</span>
             {{/each}}
        </div>
    </div>
 <div class="row">
        <div class="col-sm-12 mb-4">
            <h1 class="h3 mb-0 text-gray-800">Meme Trend  of the day</h1>
              {{#each memeOfTheDay}}
            <span class="badge badge-primary">{{memeTrendS}}</span>
            {{/each}}
        </div>
    </div>


</div>